{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "root": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "enum": [
        "brand-story",
        "basic-elements",
        "guidelines"
      ]
    },
    "requiresAuth": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "parent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "order": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sections": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "section.video-section",
        "section.page-header-section",
        "section.image-section",
        "section.editorial-text-section",
        "section.divider-section",
        "section.accordion-start",
        "section.accordion-end",
        "section.anchor-section",
        "section.small-heading-section",
        "section.card-items-section",
        "section.text-section",
        "section.stacked-card-section",
        "section.list-section",
        "section.downloads-section",
        "section.icon-link-section",
        "section.column-table",
        "section.image-slider",
        "section.image-info",
        "section.column-with-list"
      ]
    }
  }
}
